{
  "name": "@platforma-open/milaboratories.immune-assay-data.workflow",
  "version": "1.6.0",
  "type": "module",
  "description": "Tengo-based template",
  "scripts": {
    "build": "rm -rf dist && pl-tengo check && pl-tengo build",
    "format": "/usr/bin/env emacs --script ./format.el",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "dependencies": {
    "@platforma-sdk/workflow-tengo": "catalog:",
    "@platforma-open/milaboratories.immune-assay-data.prepare-fasta": "workspace:*",
    "@platforma-open/milaboratories.immune-assay-data.add-header": "workspace:*",
    "@platforma-open/milaboratories.immune-assay-data.coverage-mode-calc": "workspace:*",
    "@platforma-open/milaboratories.immune-assay-data.fasta-to-tsv": "workspace:*",
    "@platforma-open/soedinglab.software-mmseqs2": "catalog:"
  },
  "devDependencies": {
    "@platforma-sdk/tengo-builder": "catalog:"
  }
}